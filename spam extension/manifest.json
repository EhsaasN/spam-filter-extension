{
  "manifest_version": 3,
  "name": "Gmail Spam Classifier",
  "version": "1.0",
  "description": "Automatically detects and moves spam emails to the spam folder.",
  "permissions": [
    "identity",
    "alarms",
    "scripting",
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "https://www.googleapis.com/*",
    "https://mail.google.com/*",
    "http://localhost:8000/*",
    "https://spam-filter-extension-40h8lni1n-ehsaas-projects-5cc0e187.vercel.app"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "oauth2": {
    "client_id": "152609296957-cumfecvfiajk60vdq3mquog71qqhek63.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.readonly",
      "https://www.googleapis.com/auth/gmail.modify"
    ]
  },
  "key": ""
}
